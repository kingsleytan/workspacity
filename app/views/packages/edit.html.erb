<%= form_for(@package, url: packages_path, html: { id: "package-edit-form" }) do |f| %>
  <h1 class="text-center">
    Edit package
  </h1>

  <div class="form-group">
    <h4> <%= f.label :type_field, "package" %> </h4>
    <%= f.text_field :type, required: true, id: "package_type_field", class: 'form-control' %>
  </div>

  <div class="form-group text-center">
    <%= f.submit "Edit New Package", class: "btn btn-success btn-lg" %>
  </div>
<% end %>
